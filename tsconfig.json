{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015"],
    "outDir": "./ts_build",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "removeComments": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "allowJs": true
  },
  "include": [
    "scrape/**/*"
  ],
  "exclude": [
    "ts_build/**/*"
  ]
}
